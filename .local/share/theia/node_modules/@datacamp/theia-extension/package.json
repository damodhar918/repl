{
  "name": "@datacamp/theia-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "2.1.6",
  "dependencies": {
    "@theia/core": "^0.15.0",
    "@theia/editor": "^0.15.0",
    "@theia/editor-preview": "^0.15.0",
    "@theia/file-search": "^0.15.0",
    "@theia/filesystem": "^0.15.0",
    "@theia/markers": "^0.15.0",
    "@theia/terminal": "^0.15.0",
    "@theia/workspace": "^0.15.0",
    "express": "^4.16.3",
    "inversify": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@datacamp/exercise-library-config": "^1.2.4",
    "identity-obj-proxy": "3.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.0.2"
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "JEST_JUNIT_OUTPUT_NAME=$npm_package_name-result.xml yarn test --ci --reporters=default --reporters=jest-junit --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rimraf lib && mkdirp lib",
    "build": "yarn clean && yarn build:tsc",
    "prepare": "yarn build",
    "build:tsc": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/module",
      "backend": "lib/node/module"
    }
  ],
